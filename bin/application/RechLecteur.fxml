<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="200.0" prefWidth="575.0" style="-fx-background-color: #FFDAB9" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RechercheLecteurController">
    <children>
        <TextField fx:id="cinField" layoutX="108.0" layoutY="100.0" promptText="CIN" style="-fx-background-color: white;" />
        <Button fx:id="rechercheLecteurButton" layoutX="316.0" layoutY="100.0" mnemonicParsing="false" onAction="#rechercheLecteurClicked" text="Rechercher" />
        <Text fill="WHITE" layoutX="28.0" layoutY="81.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Recherche lecteur par CIN:">
         <font>
            <Font name="System Bold" size="40.0" />
         </font></Text>
        <Label fx:id="resultLabel" layoutX="15.0" layoutY="142.0" prefHeight="281.0" prefWidth="543.0" text="" textFill="#ffffff">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <Button fx:id="precButton" layoutX="454.0" layoutY="14.0" mnemonicParsing="false" onAction="#precClicked" text="Page Précédente" />
    </children>
</AnchorPane>
