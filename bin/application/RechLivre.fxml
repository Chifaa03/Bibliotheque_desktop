<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="736.0" style="-fx-background-color: #FFDAB9;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.TitreController">
    <children>
        <Label layoutX="102.0" layoutY="37.0" text="Recherche de Livre" textFill="WHITE">
            <font>
                <Font name="System Bold" size="39.0" />
            </font>
        </Label>
        <TextField fx:id="titreField" layoutX="157.0" layoutY="141.0" prefHeight="25.0" prefWidth="200.0" promptText="Entrez le titre du livre" />
        <Button fx:id="rechercherButton" layoutX="381.0" layoutY="141.0" mnemonicParsing="false" onAction="#rechercherLivre" style="-fx-background-color: white;" text="Rechercher" textFill="#9a9a9a" />
       <Label fx:id="resultLabel" layoutX="92.0" layoutY="170.0" prefHeight="281.0" prefWidth="492.0" text="" textFill="#ffffff">
            <font>
               <Font name="System Bold" size="14.0" />
            </font>
         </Label>
      <Button fx:id="precButton" layoutX="472.0" layoutY="14.0" mnemonicParsing="false" onAction="#precClicked" text="Page Précédente" />
    </children>
</AnchorPane>
