<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="771.0" style="-fx-background-color: #FFDAB9;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AffichLecteurController">
   <children>
      <Label layoutX="114.0" layoutY="30.0" text="Affichage de table Lecteur" textFill="#ffffff">
         <font>
            <Font name="System Bold" size="35.0" />
         </font>
      </Label>

      <VBox layoutX="20.0" layoutY="80.0" spacing="10">
         <TableView fx:id="lecteurTable" prefHeight="297.0" prefWidth="713.0">
            <columns>
         		<TableColumn fx:id="cinColumn" prefWidth="200.0" text="cin" />
               <TableColumn fx:id="nomColumn" prefWidth="200.0" text="nom" />
               <TableColumn fx:id="prenomColumn" prefWidth="150.0" text="prenom" />
               <TableColumn fx:id="id_aboColumn" prefWidth="150.0" text="id_abo" />
            </columns>
         </TableView>
      </VBox>
      <Button fx:id="precButton" onAction="#precClicked" layoutX="657.0" layoutY="14.0" mnemonicParsing="false" text="Page Précédente" />
   </children>
</AnchorPane>
