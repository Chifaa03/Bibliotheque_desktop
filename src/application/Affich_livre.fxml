      <Label layoutX="114.0" layoutY="30.0" text="Affichage de table Livre" textFill="#ffffff">
         <font>
            <Font name="System Bold" size="35.0" />
         </font>
      </Label>

      <VBox layoutX="20.0" layoutY="80.0" spacing="10">
         <TableView fx:id="livreTable" prefHeight="300.0" prefWidth="793.0">
            <columns>
               <TableColumn fx:id="codeColumn" prefWidth="200.0" text="code" />
               <TableColumn fx:id="titreColumn" prefWidth="200.0" text="Titre" />
               <TableColumn fx:id="auteurColumn" prefWidth="200.0" text="Auteur" />
               <TableColumn fx:id="ISBNColumn" prefWidth="150.0" text="ISBN" />
            </columns>
         </TableView>
      </VBox>
      <Button fx:id="precButton" onAction="#precClicked" layoutX="743.0" layoutY="18.0" mnemonicParsing="false" text="Page Précédente" />
   </children>
</AnchorPane>
