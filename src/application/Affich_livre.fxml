<AnchorPane prefHeight="413.0" prefWidth="829.0" style="-fx-background-color: #FFDAB9;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.TableLivreController">
